<Window x:Class="HelloSleep.LiveFeedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloSleep"
        mc:Ignorable="d"
        Title="LiveFeedWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <DockPanel>
                <TextBlock Text="Baby werd om "/>
                <TextBlock x:Name="wakkerTb" Text=""/>
            </DockPanel>
            <DockPanel>
                <TextBlock Text="Baby ging om "/>
                <TextBlock x:Name="slapenTb" Text=""/>
            </DockPanel>
            <DockPanel>
                <TextBlock Text="Baby had om "/>
                <TextBlock x:Name="TijdLaagTb"/>
                <TextBlock Text=" de laagste temperatuur van "/>
                <TextBlock x:Name="laagsteTempTb" Text=""/>
            </DockPanel>
            <DockPanel>
                <TextBlock Text="Baby had om "/>
                <TextBlock x:Name="TijdHoogTb"/>
                <TextBlock Text=" de hoogste temperatuur van "/>
                <TextBlock x:Name="hoogsteTempTb"/>
            </DockPanel>
        </StackPanel>
    </Grid>
</Window>
